# =============================================================================
# SF Zoning Compliance Automation - Environment Variables
# =============================================================================
# Copy this file to .env and fill in your API keys
# Sign up links provided below each variable

# -----------------------------------------------------------------------------
# FIRECRAWL - Web scraping & discovery
# Sign up: https://firecrawl.dev/
# -----------------------------------------------------------------------------
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# -----------------------------------------------------------------------------
# REDUCTO - PDF parsing with layout awareness
# Sign up: https://reducto.ai/
# Pipeline ID: k976v77hd0bnc5sve8860njccs808hrz
# -----------------------------------------------------------------------------
REDUCTO_API_KEY=your_reducto_api_key_here

# -----------------------------------------------------------------------------
# VOYAGE AI - Legal domain embeddings
# Sign up: https://voyageai.com/
# Model: voyage-law-2 (16k context window)
# -----------------------------------------------------------------------------
VOYAGE_API_KEY=your_voyage_api_key_here

# -----------------------------------------------------------------------------
# MONGODB ATLAS - Vector database
# Sign up: https://www.mongodb.com/atlas
# Create a cluster and get connection string
# -----------------------------------------------------------------------------
MONGODB_URI=your_mongodb_connection_string_here
MONGODB_DATABASE=sf_zoning
MONGODB_COLLECTION=zoning_chunks

# -----------------------------------------------------------------------------
# SUPABASE - Backend orchestration
# Sign up: https://supabase.com/
# Or use local dev: npx supabase init && npx supabase start
# -----------------------------------------------------------------------------
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# -----------------------------------------------------------------------------
# SF LEGISTAR - Target endpoints
# No API key required for public data
# -----------------------------------------------------------------------------
LEGISTAR_BASE_URL=https://sfgov.legistar.com
LEGISTAR_ODATA_URL=https://webapi.legistar.com/v1/sfgov

# Target file numbers for zoning compliance
TARGET_FILES=250700,250701
